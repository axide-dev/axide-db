{
    "name": "axide-db",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "browserslist": [
        "Chrome >= 92",
        "Firefox >= 91",
        "Safari >= 15",
        "Edge >= 92"
    ],
    "scripts": {
        "build": "next build && opennextjs-cloudflare build --skip-build",
        "check": "next lint && tsc --noEmit",
        "dev": "next dev",
        "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
        "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
        "lint": "next lint",
        "lint:fix": "next lint --fix",
        "preview": "bun run build && opennextjs-cloudflare preview",
        "deploy": "bun run build && opennextjs-cloudflare deploy",
        "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
        "start": "next start",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "@base-ui/react": "^1.0.0",
        "@clerk/nextjs": "^6.36.5",
        "@hugeicons/core-free-icons": "^3.1.1",
        "@hugeicons/react": "^1.1.4",
        "@opennextjs/cloudflare": "^1.14.7",
        "@t3-oss/env-nextjs": "^0.13.10",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "convex": "^1.31.2",
        "next": "^15.5.9",
        "radix-ui": "^1.4.3",
        "react": "^19.2.3",
        "react-dom": "^19.2.3",
        "shadcn": "^3.6.2",
        "tailwind-merge": "^3.4.0",
        "tw-animate-css": "^1.4.0",
        "zod": "^4.3.5"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.3.3",
        "@tailwindcss/postcss": "^4.1.18",
        "@types/node": "^25.0.3",
        "@types/react": "^19.2.7",
        "@types/react-dom": "^19.2.3",
        "babel-plugin-react-compiler": "^1.0.0",
        "eslint": "^9.39.2",
        "eslint-config-next": "^16.1.1",
        "postcss": "^8.5.6",
        "prettier": "^3.7.4",
        "prettier-plugin-tailwindcss": "^0.7.2",
        "tailwindcss": "^4.1.18",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.51.0",
        "wrangler": "^4.54.0"
    },
    "ct3aMetadata": {
        "initVersion": "7.40.0"
    }
}